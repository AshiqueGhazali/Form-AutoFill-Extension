{
    "manifest_version": 3,
    "name": "Google Form Autofill",
    "version": "1.0",
    "permissions": [
        "activeTab",
        "scripting"
    ],
    "action": {
        "default_popup": "index.html"
    },
    "content_scripts": [
        {
            "matches": ["https://docs.google.com/forms/*"],
            "js": ["content.js"]
        }
    ]
}
